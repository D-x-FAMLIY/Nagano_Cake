<div class="container">
  <div class="row">
    <div class="col-md-3">
      <table>
        <tr>
          <th>ジャンル検索</th>
        </tr>
        <% @genres.each do |genre| %>
          <tr>
            <td><%= genre.name %></td>
          </tr>
        <% end %>
      </table>
    </div>
    <div class="col-md-9">
      <%= product.name %>
      <%= product.introduction %>
      <%= product.proce_without_tax %>
      <%= f.submit "カートに入れる", class: 'btn btn-sm btn-success' %>
    </div>
  </div>
</div>